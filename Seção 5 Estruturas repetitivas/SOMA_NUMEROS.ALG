Algoritmo "Soma_Numeros"

Var

  N, i, x, soma  : inteiro

Inicio


  Escreval("Quantos numeros serão digitados: ")
  Leia(N)
  
  Soma <- 0
  
    Para i de 1 até N faca
      Escreval("Digite um numero: ")
      Leia (X)
      Soma <- soma + X
    Fimpara
    
    Escreva("SOMA: ",soma)
    



Fimalgoritmo